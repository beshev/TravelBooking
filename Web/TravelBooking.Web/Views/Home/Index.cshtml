@{
    this.ViewData[GlobalConstants.Title] = "Начало";
}

<!-- Home -->
<div id="header">
    <div class="flexslider">
        <ul class="slides">
            <li class="slider-item text-center">
                <div class="intro container">
                    <div class="inner-intro">
                        <h1 class="header-title">
                            <span>пътуване</span>
                        </h1>

                        <a asp-action="Travel" asp-controller="Bookings" class="btn custom-btn">
                            запази
                        </a>
                    </div>
                </div>
            </li>
            <li class="slider-item">
                <div class="intro">
                    <div class="inner-intro">
                        <h1 class="header-title">
                            <span>багаж</span>
                        </h1>
                        <a asp-action="Baggage" asp-controller="Bookings" class="btn custom-btn">
                            изпрати
                        </a>
                    </div>
                </div>
            </li>
            <li class="slider-item">
                <div class="intro">
                    <div class="inner-intro">
                        <h1 class="header-title">
                            <span>Животно</span>
                        </h1>
                        <a asp-action="Animal" asp-controller="Bookings" class="btn custom-btn">
                            изпрати
                        </a>
                    </div>
                </div>
            </li>
            <li class="slider-item">
                <div class="intro">
                    <div class="inner-intro">
                        <h1 class="header-title">
                            <span>мотор</span>
                        </h1>
                        <a asp-action="Vehicle" asp-controller="Bookings" class="btn custom-btn">
                            изпрати
                        </a>
                    </div>
                </div>
            </li>
            <!-- /.slider-item -->
        </ul> <!-- /.slides -->
    </div> <!-- /.flexslider -->
</div> <!-- /#header -->

<!-- Top place to visit -->
<section class="visit section-wrapper">
    <div class="container">
        <h2 class="section-title">
            Top place to visit
        </h2>
        <p class="section-subtitle">
            Lorem Ipsum is simply dummy text of the industry.
        </p>

        <div class="owl-carousel visit-carousel" id="">
            <div class="item">
                <img src="./images/visit-1.png" alt="visit-image" class="img-responsive visit-item">
            </div>
            <div class="item">
                <img src="./images/visit-2.png" alt="visit-image" class="img-responsive visit-item">
            </div>
            <div class="item">
                <img src="./images/visit-3.png" alt="visit-image" class="img-responsive visit-item">
            </div>
            <div class="item">
                <img src="./images/visit-1.png" alt="visit-image" class="img-responsive visit-item">
            </div>
            <div class="item">
                <img src="./images/visit-2.png" alt="visit-image" class="img-responsive visit-item">
            </div>
            <div class="item">
                <img src="./images/visit-3.png" alt="visit-image" class="img-responsive visit-item">
            </div>
            <div class="item">
                <img src="~/images/test.jpg" alt="visit-image" class="img-responsive visit-item" style="height: 408px;">
            </div>
        </div>
    </div> <!-- /.container -->
</section> <!-- /.visit -->

